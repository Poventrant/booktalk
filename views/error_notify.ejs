<%- include ./templ/header %>

<div class="container">
	<div class="row clearfix">
		<div class="col-md-12 column">
			<div class="row clearfix">
				<div class="col-md-8 column">
					<div class="panel panel-default">
						<div class="panel-heading">
						 	<h3 class="panel-title">
                 错误提示
              </h3> 
						</div>
						<div class="panel-body">
				      <% if(typeof(error) !== 'undefined' && error){ %>
				      <div class="alert alert-error">
				        <strong><%= error %></strong>
				      </div>
				      <% } %>
				      <% if(typeof(success) !== 'undefined' && success){ %>
				      <div class="alert alert-success">
				        <strong><%= success %></strong>
				      </div>
				      <% } %>
				      <a href="<%- typeof(referer) !== 'undefined' ? referer : '/' %>">
				      	<input type="submit" name="change" class="btn btn-primary" value="返回" />
			      	</a> 
						</div> 
					</div>  
				</div>
				<div class="col-md-4 column">
					<div class="panel panel-default">
						<div class="panel-heading">
							<h3 class="panel-title">
								关于我们
							</h3>
						</div>
						<div class="panel-body">
							<%= config.about %>
						</div> 
					</div>
					<div class="panel panel-default">
						<div class="panel-heading">
							<h3 class="panel-title">
								联系方式
							</h3>
						</div>
						<div class="panel-body">
							<ul>
								<li>联系人:潘先生</li>
								<li>地址:中山大学至善园四号515</li>
								<li>电话:18814091214</li> 
								<li>Email:406419317@qq.com</li>
							</ul>
						</div> 
					</div>					
			</div> 
		</div>
	</div>
</div>
 
<%- include ./templ/footer %>
 